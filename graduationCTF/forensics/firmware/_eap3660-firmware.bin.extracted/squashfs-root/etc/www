 + " " + share.err) );}function setMemoryValues(val) {var mem = val.replace(/'/g, "").split(",");var memTotal = parseInt(mem[19]) / 1024;var memSystem = Math.pow(2, Math.ceil(Math.log(memTotal) / Math.LN2));var memFree = parseInt(mem[22]) / 1024;var memUsed = memTotal - memFree;var memBuffer = parseInt(mem[28]) / 1024;var memCached = parseInt(mem[31]) / 1024;var memActive = parseInt(mem[37]) / 1024;var memInactive = parseInt(mem[40]) / 1024;setElementContent("mem_total", memTotal.toFixed(1) + " MiB / " + memSystem.toFixed(1) + " MiB");setElementContent("mem_free", memFree.toFixed(1) + " MiB / " + memTotal.toFixed(1) + " MiB");setElementContent("mem_used", memUsed.toFixed(1) + " MiB / " + memTotal.toFixed(1) + " MiB");setElementContent("mem_buffer", memBuffer.toFixed(1) + " MiB / " + memUsed.toFixed(1) + " MiB");setElementContent("mem_cached", memCached.toFixed(1) + " MiB / " + memUsed.toFixed(1) + " MiB");setElementContent("mem_active", memActive.toFixed(1) + " MiB / " + memUsed.toFixed(1) + " MiB");setElementContent("mem_inactive", memInactive.toFixed(1) + " MiB / " + memUsed.toFixed(1) + " MiB");}var update;addEvent(window, "load", function() {setPacketInfo("<% wl_packet_get(); %>");setMemoryValues("<% dumpmeminfo(); %>");<% ifndef("HASWIFI", "/"); %><% ifndef("HASWIFI", "*"); %>setWirelessTable(<% active_wireless(1); %>);setWDSTable(<% active_wds(1); %>);<% ifndef("HASWIFI", "*"); %><% ifndef("HASWIFI", "/"); %>setDHCPTable(<% dumpleases(1); %>);setElementVisible("dhcp", "<% nvg("lan_proto"); %>" == "dhcp");setElementVisible("ipv6", "<% nvg("ipv6_enable"); %>" == "1");update = new StatusUpdate("Info.live.htm", <% nvg("refresh_time"); %>);update.onUpdate("packet_info", function(u) {setPacketInfo(u.packet_info);});update.onUpdate("mem_info", function(u) {setMemoryValues(u.mem_info);});update.onUpdate("active_wireless", function(u) {eval('setWirelessTable(' + u.active_wireless + ')');});update.onUpdate("active_wds", function(u) {eval('setWDSTable(' + u.active_wds + ')');});update.onUpdate("dhcp_leases", function(u) {eval('setDHCPTable(' + u.dhcp_leases + ')');});update.onUpdate("lan_proto", function(u) {setElementVisible("dhcp", u.lan_proto == "dhcp");});update.start();});addEvent(window, "unload", function() {update.stop();});function refresh(F){F.submit();}

zoom)),new Array(1,0,0,camera.panX,0,1,0,camera.panY,0,0,1,camera.panZ,0,0,0,1)),mtx=mult_mtx(view_mtx,model_mtx)}function rotate_mtx(e,t,a,n){var i=Math.sin(n),o=Math.cos(n),r=1-o;return new Array(e*e*r+o,e*t*r-a*i,e*a*r+t*i,0,t*e*r+a*i,t*t*r+o,t*a*r-e*i,0,e*a*r-t*i,t*a*r+e*i,a*a*r+o,0,0,0,0,1)}function postrotate(e,t,a,n){model_mtx=mult_mtx(rotate_mtx(e,t,a,n),model_mtx),mtx=mult_mtx(view_mtx,model_mtx)}function rotate(e,t,a,n){model_mtx=mult_mtx(model_mtx,rotate_mtx(e,t,a,n)),mtx=mult_mtx(view_mtx,model_mtx)}function scale_mtx(e){return new Array(e,0,0,0,0,e,0,0,0,0,e,0,0,0,0,1)}function scale(e){model_mtx=mult_mtx(scale_mtx(e),model_mtx),mtx=mult_mtx(view_mtx,model_mtx)}function translate(e,t,a){model_mtx[3]+=e,model_mtx[7]+=t,model_mtx[11]+=a}function init_3d(){var e,t,a,n;items=new Array;for((mainarea=document.getElementById("mainarea")).innerHTML="",t=-.75;t<=.75;t+=.75)for(a=-.75;a<=.75;a+=.75)for(n=-.75;n<=.75;n+=.75)(e=new Object).x=t,e.y=a,e.z=n,e.visible=!0,e.div=document.createElement("div"),e.div.className="gridpoint",mainarea.appendChild(e.div),items[items.length]=e;screenH=mainarea.clientHeight,screenW=mainarea.clientWidth,loadBasicMatrix(),globalRedraw=!0,setTimeout("tick()",10),updatePrefs(),(logodiv=document.getElementById("logo")).style.left="30%",logodiv.style.top="30%",movePropertyTo("logodiv.style.left",0,"%",.6,.01,!0,!0),movePropertyTo("logodiv.style.top",0,"%",.6,.01,!0,!0)}function mult_mtx(e,t){var a,n;for(mtxo=new Array(16),a=0;a<4;a++)for(n=0;n<4;n++)mtxo[4*a+n]=e[4*a]*t[n]+e[4*a+1]*t[n+4]+e[4*a+2]*t[n+8]+e[4*a+3]*t[n+12];return mtxo}function tick(){if(now=(new Date).getTime(),prefs.rotate&&movePropertyTo("camera.azimuth",parseFloat(camera.azimuth)+.02,"",.1,.02,!0,!1),tickMoveStep&&moveStep(),(wvzFrame=wvzFrame||document.getElementById("wivizGetFrame"))&&5e3<now-lastWvzUpdate){var e=wvzFrame.contentWindow.location;demomode&&(wvzLoc=demourl+demoindex+".html",demomax<++demoindex&&(demoindex=1)),e.href==wvzLoc?e.reload(!0):e.replace(wvzLo